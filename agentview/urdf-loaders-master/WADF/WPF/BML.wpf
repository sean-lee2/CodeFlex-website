<?xml version="1.0" ?>
<WorkcellPerformances>

    <Performance PerformanceName="CycleTime">
        <Measure MeasureName="ProcessStartTime">
            <DataReference Edge="Falling">ProximitySensorIn/Monitoring/ProximitySensorIn_monitoring_state_arg</DataReference>
        </Measure>
        <Measure MeasureName="ProcessEndTime">
            <DataReference Edge="Falling">ProximitySensorOut/Monitoring/ProximitySensorOut_monitoring_state_arg</DataReference>
        </Measure>
        <Formula>(t1 - t0).total_seconds() if (t1 - t0).total_seconds() > 0 else None</Formula>
    </Performance>

    <Performance PerformanceName="IdleTime" Update="Accumulate">
        <Measure MeasureName="ProcessEndTime">
            <DataReference Edge="Falling">ProximitySensorOut/Monitoring/ProximitySensorOut_monitoring_state_arg</DataReference>
        </Measure>
        <Measure MeasureName="ProcessStartTime">
            <DataReference Edge="Falling">ProximitySensorIn/Monitoring/ProximitySensorIn_monitoring_state_arg</DataReference>
        </Measure>
        <Formula>(t1 - t0) if (t1 - t0).total_seconds() > 0 else None</Formula>
    </Performance>

    <Performance PerformanceName="Productivity">
        <Measure MeasureName="TotalProductionQuantity">
            <Value>100</Value>
        </Measure>
        <Measure MeasureName="CurrentProductionQuantity" Update="Increment">
            <DataReference Edge="Falling">ProximitySensorOut/Monitoring/ProximitySensorOut_monitoring_state_arg</DataReference>
        </Measure>
        <Formula>(v1 / v0) * 100 if v0 > 0 else None</Formula>
    </Performance>

</WorkcellPerformances>
